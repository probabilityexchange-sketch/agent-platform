{
    "version": 2,
    "installCommand": "npm install --legacy-peer-deps",
    "buildCommand": "bash scripts/vercel-build.sh",
    "framework": "nextjs",
    "regions": [
        "iad1"
    ],
    "crons": [
        {
            "path": "/api/cron/cleanup",
            "schedule": "*/10 * * * *"
        },
        {
            "path": "/api/cron/expire-pending",
            "schedule": "0 * * * *"
        }
    ]
}
